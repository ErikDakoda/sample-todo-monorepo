{
  "name": "zenstack-sample-todo-nextjs-turbo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.*"
  },
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "pnpm:install": "pnpm install",
    "prisma:studio": "pnpm -F @erikdakoda/database prisma:studio",
    "prisma:push": "pnpm -F @erikdakoda/database prisma:push",
    "prisma:push-hard": "pnpm -F @erikdakoda/database prisma:push-hard",
    "prisma:symlinks": "pnpm -F @erikdakoda/database prisma:symlinks",
    "zen:generate": "pnpm -F @erikdakoda/database zen:generate",
    "clean": "turbo run clean && rm -rf node_modules/",
    "build": "turbo run build",
    "build-quick": "turbo run build-quick",
    "dev": "turbo run dev",
    "debug": "turbo run debug",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ts:check": "turbo run ts:check",
    "vitest:symlinks": "bash scripts/vitest-symlinks.sh",
    "vitest:unit": "vitest unit",
    "vitest:integ": "vitest integ",
    "vitest:coverage": "turbo run vitest:coverage",
    "npm:version": "turbo run npm:version -- 0.1.0",
    "npm:check-update": "turbo run npm:check-update",
    "npm:check-unused": "turbo run npm:check-unused",
    "syncpack:list": "syncpack list-mismatches",
    "syncpack:fix": "syncpack fix-mismatches",
    "syncpack:update": "syncpack update --types 'prod,dev'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "default-configs": "workspace:*",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-turbo": "1.12.5",
    "eslint-plugin-jsdoc": "48.2.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "syncpack": "12.3.0",
    "turbo": "1.12.5",
    "typescript": "5.4.2",
    "vitest": "1.4.0"
  }
}
